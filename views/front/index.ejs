                           <li class="nav-item">
                              <a href="#h1-tab-products-3" data-toggle="tab">Burgers</a>
                           </li>
                        </ul>
                        <div class="tab-content">
                           <div class="tab-pane" id="h1-tab-products-1">
                              <div class="section-products">
                                 <div class="woocommerce columns-3">
                                    <div class="columns-3">
                                       <ul class="products">
                                          <% let lim = 0 %>
                                          <% data.products.forEach((item, index) => { %>
                                             <% if(item.categoryId === '6134443f412c447908f18be5' && lim < 3) { %>
                                                   <% lim++ %>
                                                   <li class="product">
                                                   <div class="product-outer">
                                                      <div class="product-inner">
                                                         <div class="product-image-wrapper">
                                                            <a href="/item/<%- item._id %>" class="woocommerce-LoopProduct-link">
                                                            <img src="<%- item.image %>" class="img-responsive" alt="">
                                                            </a>
                                                         </div>
                                                         <div class="product-content-wrapper">
                                                            <a href="/item/<%- item._id %>" class="woocommerce-LoopProduct-link">
                                                               <h3><%- item.name %></h3>
                                                               <div itemprop="description">
                                                                  <p style="max-height: none;"><%- item.description %></p>
                                                               </div>
                                                            </a>
                                                         </div>
                                                      </div>
                                                   </div>
                                                </li>
                                             <% } %>
                                          <% }) %>
                                          <% lim = 0 %>
                                       </ul>
                                    </div>
                                 </div>
                              </div>
                           </div>
