<%- include('partials/head.ejs') %> 
<body class="left-sidebar list-view columns-2 archive woocommerce-page lite">
  <div id="page" class="hfeed site">
    <%- include('partials/navbar.ejs') %> 
     <div id="content" class="site-content" tabindex="-1">
        <div class="col-full">
           <div class="pizzaro-sorting">
              <form class="woocommerce-ordering" method="get">
                 <select name="orderby" class="orderby">
                    <option value="menu_order" selected="selected">Default sorting</option>
                    <option value="popularity">Sort by popularity</option>
                    <option value="rating">Sort by average rating</option>
                    <option value="date">Sort by newness</option>
                    <option value="price">Sort by price: low to high</option>
                    <option value="price-desc">Sort by price: high to low</option>
                 </select>
                 <input type="hidden" name="shop_view" value="grid-view"><input type="hidden" name="shop_style" value="lite"><input type="hidden" name="shop_layout" value="left-sidebar"><input type="hidden" name="shop_columns" value="3">
              </form>
              <div class="product-filters-widgets">
                 <div class="collapse" id="products_filter_collapse">
                    <div id="woocommerce_price_filter-2" class="widget woocommerce widget_price_filter">
                       <span class="gamma widget-title">Filter by price</span>
                       <form method="get" action="shop-sidebar-list-2-column.html#">
                          <div class="price_slider_wrapper">
                             <div class="price_slider ui-slider ui-slider-horizontal ui-widget ui-widget-content ui-corner-all">
                                <div class="ui-slider-range ui-widget-header ui-corner-all" style="left: 0%; width: 100%;"></div>
                                <span class="ui-slider-handle ui-state-default ui-corner-all" tabindex="0" style="left: 0%;"></span><span class="ui-slider-handle ui-state-default ui-corner-all" tabindex="0" style="left: 100%;"></span>
                             </div>
                             <div class="price_slider_amount">


                                <button type="submit" class="button">Filter</button>
                                <div class="price_label">Price: <span class="from">$1</span> â€” <span class="to">$59</span></div>
                                <div class="clear"></div>
                             </div>
                          </div>
                       </form>
                    </div>
                    <div id="woocommerce_layered_nav-2" class="widget woocommerce widget_layered_nav">
                       <span class="gamma widget-title">Food Type</span>
                       <ul>
                          <li class="wc-layered-nav-term "><a href="shop-sidebar-list-2-column.html#">Meat</a> <span class="count">(7)</span></li>
                          <li class="wc-layered-nav-term "><a href="shop-sidebar-list-2-column.html#">Spicy</a> <span class="count">(7)</span></li>
                          <li class="wc-layered-nav-term "><a href="shop-sidebar-list-2-column.html#">Veg</a> <span class="count">(12)</span></li>
                       </ul>
                    </div>
                 </div>
              </div>
           </div>
           <div id="primary" class="content-area">
              <main id="main" class="site-main" >
                 <ul class="products">
                    <% data.menuData.forEach((product, index) => { %>
                     <li class="product pa_food-type-veg <%- index%2===0 ? 'first' : 'last' %>">
                        <div class="product-outer">
                           <div class="product-inner">
                              <div class="product-image-wrapper">
                                 <a href="/item/<%- product._id %>" class="woocommerce-LoopProduct-link"><img width="300" height="300" src="<%- product.image %>" class="attachment-shop_catalog size-shop_catalog wp-post-image" alt="13" title="13"></a>
                              </div>
                              <div class="product-content-wrapper">
                                 <a href="/item/<%- product._id %>" class="woocommerce-LoopProduct-link">
                                    <h3><%- product.name %></h3>
                                    <div itemprop="description">
                                       <p style="max-height: none;"><%- product.description %></p>
                                    </div>
                                    <div  class="yith_wapo_groups_container">
                                       <div  class="ywapo_group_container ywapo_group_container_radio form-row form-row-wide " data-requested="1" data-type="radio" data-id="1" data-condition="">
                                          <div class="ywapo_input_container ywapo_input_container_radio">
                                             <span class="ywapo_label_price">
                                                <span class="woocommerce-Price-amount amount">
                                                <span class="woocommerce-Price-currencySymbol">$</span><%- product.price.toFixed(2) %></span>
                                             </span>
                                          </div>
                                       </div>
                                    </div>
                                 </a>
                                 <div class="hover-area"><a rel="nofollow" href="single-product-v1.html" data-quantity="1" data-product_id="82" data-product_sku="" class="button product_type_simple add_to_cart_button ajax_add_to_cart">Add to cart</a></div>
                              </div>
                           </div>
                           <!-- /.product-inner -->
                        </div>
                        <!-- /.product-outer -->
                     </li>
                    <% }); %>
                 </ul>
              </main>
              <!-- #main -->
           </div>
           <!-- #primary -->
           <div id="secondary" class="widget-area" role="complementary">
              <div id="nav_menu-2" class="widget widget_nav_menu">
                 <div class="menu-food-menu-container">
                    <ul id="menu-food-menu-2" class="menu">
                       <li class="menu-item"><a href="/menu/pizza"><i class="po po-pizza"></i>Pizza</a></li>
                       <li class="menu-item"><a href="/menu/burgers"><i class="po po-burger"></i>Burgers</a></li>
                       <li class="menu-item"><a href="/menu/salads"><i class="po po-salads"></i>Salads</a></li>
                       <li class="menu-item"><a href="/menu/tacos"><i class="po po-tacos"></i>Tacos</a></li>
                       <li class="menu-item"><a href="/menu/wraps"><i class="po po-wraps"></i>Wraps</a></li>
                       <li class="menu-item"><a href="/menu/fries"><i class="po po-fries"></i>Fries</a></li>
                       <li class="menu-item"><a href="/menu/salads"><i class="po po-salads"></i>Salads</a></li>
                       <li class="menu-item"><a href="/menu/drinks"><i class="po po-drinks"></i>Drinks</a></li>
                    </ul>
                 </div>
              </div>
              <div id="search-2" class="widget widget_search">
                 <form role="search" method="get" class="search-form" action="blog-single.html">
                    <label>
                    <span class="screen-reader-text">Search for:</span>
                    <input type="search" class="search-field" placeholder="Search &hellip;" value="" name="s" />
                    </label>
                    <input type="submit" class="search-submit" value="Search" />
                 </form>
              </div>
           </div>
           <!-- #secondary -->
        </div>
        <!-- .col-full -->
     </div>
  </div>
<%- include('partials/footer.ejs') %>
<%- include('partials/script.ejs') %> 
